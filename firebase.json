{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/s3/**",
        "destination": "/dk64r-tracker.html"
      },
      {
        "source": "/s3-race/**",
        "destination": "/dk64r-race-tracker.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
